$Function Диаметр_класса : real
$Type = algorithmic
$Parameters
  размер_1     : integer
  размер_2     : integer
  класс        : such_as Позиция_заказа.класс
$Body
  Calculate_if класс = S  Диаметр_класса = (размер_1 + размер_2) * 0.7 + IAbs(размер_1 - размер_2) / 10.0
  Calculate_if класс = A  Диаметр_класса = (размер_1 + размер_2) * 0.7
  Calculate_if класс = B  Диаметр_класса = (размер_1 + размер_2) * 0.6
  Calculate_if класс = C  Диаметр_класса = IMax(размер_1, размер_2) + 3.0
$End

$Function Требуемый_диаметр : real
$Type = algorithmic
$Parameters
  типоразмер     : such_as Позиция_заказа.типоразмер
  класс          : such_as Позиция_заказа.класс
  кол_пилоблоков : integer [1..9]
$Body
  Calculate_if кол_пилоблоков = 1  Требуемый_диаметр =
    Диаметр_класса(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер), класс)
  Calculate_if кол_пилоблоков = 2  Требуемый_диаметр =
    Диаметр_класса(IMin(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)) * 2,
                   IMax(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)), класс)
  Calculate_if кол_пилоблоков = 3  Требуемый_диаметр =
    Диаметр_класса(IMin(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)) * 3,
                   IMax(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)), класс)
  Calculate_if кол_пилоблоков = 4  Требуемый_диаметр =
    Диаметр_класса(Размеры_пиломатериала(_ширина, типоразмер) * 2,
                   Размеры_пиломатериала(_высота, типоразмер) * 2, класс)
  Calculate_if кол_пилоблоков = 5 or кол_пилоблоков = 6 Требуемый_диаметр =
    Диаметр_класса(IMin(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)) * 3,
                   IMax(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)) * 2, класс)
  Calculate_if кол_пилоблоков = 7 or кол_пилоблоков = 8 Требуемый_диаметр =
    Диаметр_класса(IMin(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)) * 4,
                   IMax(Размеры_пиломатериала(_ширина, типоразмер), Размеры_пиломатериала(_высота, типоразмер)) * 2, класс)
  Calculate_if кол_пилоблоков = 9  Требуемый_диаметр =
    Диаметр_класса(Размеры_пиломатериала(_ширина, типоразмер) * 3,
                   Размеры_пиломатериала(_высота, типоразмер) * 3, класс)
$End

$Function Получить_кол_брусьев : integer [0..9]
$Type = algorithmic
$Parameters
  типоразмер  : such_as Позиция_заказа.типоразмер
  класс       : such_as Позиция_заказа.класс
  диаметр     : real
$Body
  Calculate_if диаметр <  Требуемый_диаметр(типоразмер, класс, 1)  Получить_кол_брусьев = 0
  Calculate_if диаметр >= Требуемый_диаметр(типоразмер, класс, 9)  Получить_кол_брусьев = 9
  Calculate_if диаметр >= Требуемый_диаметр(типоразмер, класс, 8)  Получить_кол_брусьев = 8
  Calculate_if диаметр >= Требуемый_диаметр(типоразмер, класс, 6)  Получить_кол_брусьев = 6
  Calculate_if диаметр >= Требуемый_диаметр(типоразмер, класс, 4)  Получить_кол_брусьев = 4
  Calculate_if диаметр >= Требуемый_диаметр(типоразмер, класс, 3)  Получить_кол_брусьев = 3
  Calculate_if диаметр >= Требуемый_диаметр(типоразмер, класс, 2)  Получить_кол_брусьев = 2
  Calculate_if диаметр >= Требуемый_диаметр(типоразмер, класс, 1)  Получить_кол_брусьев = 1
$End

