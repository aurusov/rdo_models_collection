$Function Конв_шаг : integer
$Type = algorithmic
$Parameters
  код       : such_as Бревно.код_места
$Body
  Calculate_if код = Тр_1  Конв_шаг = Шаг_трапспортера
  Calculate_if код = Тр_2  Конв_шаг = Шаг_трапспортера
  Calculate_if код = Тр_3  Конв_шаг = Шаг_трапспортера
  Calculate_if код = Кн_1  Конв_шаг = Конвейер_1.шаг
  Calculate_if код = Кн_2  Конв_шаг = Конвейер_2.шаг
  Calculate_if код = Кн_3  Конв_шаг = Конвейер_3.шаг
  Calculate_if код = Кн_4  Конв_шаг = Конвейер_4.шаг
  Calculate_if код = Кн_5  Конв_шаг = Конвейер_5.шаг
$End

$Function Общая_длина : integer
$Type = algorithmic
$Parameters
  код       : such_as Бревно.код_места
$Body
  Calculate_if код = Тр_1  Общая_длина = Транспортер_1.длина
  Calculate_if код = Тр_2  Общая_длина = Транспортер_2.длина
  Calculate_if код = Тр_3  Общая_длина = Транспортер_3.длина
  Calculate_if код = Кн_1  Общая_длина = Конвейер_1.длина
  Calculate_if код = Кн_2  Общая_длина = Конвейер_2.длина
  Calculate_if код = Кн_3  Общая_длина = Конвейер_3.длина
  Calculate_if код = Кн_4  Общая_длина = Конвейер_4.длина
  Calculate_if код = Кн_5  Общая_длина = Конвейер_5.длина
$End

$Function Новый_счетчик_оборотов : integer
$Type = algorithmic
$Parameters
  направление  : such_as Конвейер.статус
  позиция      : integer
  код          : such_as Бревно.код_места
$Body
  Calculate_if направление = вперед and позиция - Конв_шаг(код) < 0
    Новый_счетчик_оборотов = 1
  Calculate_if направление = вперед and позиция - Конв_шаг(код) >= 0
    Новый_счетчик_оборотов = 0
  Calculate_if направление = назад and позиция + Конв_шаг(код) < Общая_длина(код)
    Новый_счетчик_оборотов = 0
  Calculate_if направление = назад and позиция + Конв_шаг(код) >= Общая_длина(код)
    Новый_счетчик_оборотов = -1
$End

$Function Новая_позиция : integer
$Type = algorithmic
$Parameters
  направление  : such_as Конвейер.статус
  позиция      : integer
  код          : such_as Бревно.код_места
$Body
  Calculate_if направление = вперед and позиция - Конв_шаг(код) < 0
    Новая_позиция = позиция - Конв_шаг(код) + Общая_длина(код)
  Calculate_if направление = вперед and позиция - Конв_шаг(код) >= 0
    Новая_позиция = позиция - Конв_шаг(код)
  Calculate_if направление = назад and позиция + Конв_шаг(код) < Общая_длина(код)
    Новая_позиция = позиция + Конв_шаг(код)
  Calculate_if направление = назад and позиция + Конв_шаг(код) >= Общая_длина(код)
    Новая_позиция = позиция + Конв_шаг(код) - Общая_длина(код)
$End

$Function Новый_счетчик_оборотов_бревно : integer
$Type = algorithmic
$Parameters
  позиция         : integer
  длина_пилоблока : integer
  длина           : integer
$Body
  Calculate_if позиция - длина_пилоблока >= 0  Новый_счетчик_оборотов_бревно = 0
  Calculate_if позиция - длина_пилоблока  < 0  Новый_счетчик_оборотов_бревно = 1
$End

$Function Новая_позиция_бревно : integer
$Type = algorithmic
$Parameters
  позиция         : integer
  длина_пилоблока : integer
  длина           : integer
$Body
  Calculate_if позиция - длина_пилоблока >= 0
    Новая_позиция_бревно = позиция - длина_пилоблока
  Calculate_if позиция - длина_пилоблока  < 0
    Новая_позиция_бревно = позиция - длина_пилоблока + длина
$End

$Function X_поз_Ф1 : integer
$Type = algorithmic
$Parameters
  код              : such_as Бревно.код_места
  позиция          : integer
  счетчик_оборотов : integer
$Body
  Calculate_if код = Тр_3  X_поз_Ф1 = 78
  Calculate_if код = Кн_1
    X_поз_Ф1 = 440+(50 + Конвейер_1.позиция - позиция -
      (Конвейер_1.счетчик_оборотов - счетчик_оборотов - 1) * Конвейер_1.длина) / 10
  Calculate_if код = Кн_2
    X_поз_Ф1 = 70+(1000 + 50 + Конвейер_2.позиция - позиция -
      (Конвейер_2.счетчик_оборотов - счетчик_оборотов - 1) * Конвейер_2.длина) / 10
  Calculate_if код = Кн_3
    X_поз_Ф1 = 60+(50 + Конвейер_3.позиция - позиция -
      (Конвейер_3.счетчик_оборотов - счетчик_оборотов - 1) * Конвейер_3.длина) / 10
  Calculate_if код = Кн_4
    X_поз_Ф1 = 60-(Конвейер_4.позиция - позиция - 50 -
      (Конвейер_4.счетчик_оборотов - счетчик_оборотов) * Конвейер_4.длина) / 10
  Calculate_if код = Кн_5
    X_поз_Ф1 = 80-(Конвейер_5.позиция - позиция - 50 - 1000 -
      (Конвейер_5.счетчик_оборотов - счетчик_оборотов) * Конвейер_5.длина) / 10
$End

