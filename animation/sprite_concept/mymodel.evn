$Pattern Образец_прихода_клиента : event
$Relevant_resources
	_Парикмахерская: Парикмахерская Keep
	_Клиент        : Клиенты        Create
$Body
_Парикмахерская
	Convert_event
		Образец_прихода_клиента.planning( time_now + Интервал_прихода( 30 ) );
		количество_в_очереди++;

_Клиент
	Convert_event trace
		тип       = Тип_клиента;
		состояние = Пришел;
$End
